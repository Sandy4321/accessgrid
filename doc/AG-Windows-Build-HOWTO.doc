\par reg.exe query HKLM\\SOFTWARE\\Microsoft\\VisualStudio\\7.0\\Setup\\VS /v EnvironmentDirectory > msid.txt
\par for /F "skip=1 tokens=3*" %%i IN (msid.txt) DO set VSINSTALLDIR=%%i %%j
\par if exist msid.txt del msid.txt
\par set VCINSTALLDIR=
\par reg.exe query HKLM\\SOFTWARE\\Microsoft\\VisualStudio\\7.0\\Setup\\VS /v ProductDir > msid.txt
\par for /F "skip=1 tokens=3*" %%i IN (msid.txt) DO set VCINSTALLDIR=%%i %%j
\par if exist msid.txt del msid.txt
\par }{\rtlch\fcs1 \af2 \ltrch\fcs0 \insrsid7481329 
