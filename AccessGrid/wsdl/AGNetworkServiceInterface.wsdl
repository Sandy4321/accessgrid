<?xml version="1.0" encoding="UTF-8"?>
<definitions 
  xmlns="http://schemas.xmlsoap.org/wsdl/"
  xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
  xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/"
  xmlns:http="http://schemas.xmlsoap.org/wsdl/http/"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns:agtk="http://www.accessgrid.org/v3.0"
  xmlns:agnets="http://www.accessgrid.org/v3.0/agnetworkservice"
  targetNamespace="http://www.accessgrid.org/v3.0/agnetworkservice">

      <types>
      <schema targetNamespace="http://www.accessgrid.org/v3.0/agnetworkservice" 
              elementFormDefault="qualified" 
	      attributeFormDefault="unqualified" 
              xmlns="http://www.w3.org/2001/XMLSchema">

           <xsd:element name="TransformRequest">
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element minOccurs="0" maxOccurs="unbounded" name="streamDescription" type="agtk:StreamDescription" />
              </xsd:sequence>
            </xsd:complexType>
	  </xsd:element>

          <xsd:element name="TransformResponse">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element minOccurs="0" maxOccurs="unbounded" name="streamDescription" type="agtk:StreamDescription" />
               </xsd:sequence>
            </xsd:complexType>
          </xsd:element>

          <xsd:element name="Transform3Request">
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element minOccurs="0" maxOccurs="unbounded" name="streamDescription" type="agtk:StreamDescription3" />
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>

	  <xsd:element name="Transform3Response">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element minOccurs="0" maxOccurs="unbounded" name="streamDescription" type="agtk:StreamDescription3" />
               </xsd:sequence>
            </xsd:complexType>
          </xsd:element>


          <xsd:element name="StopTransformRequest">
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element minOccurs="0" maxOccurs="unbounded" name="streamDescription" type="agtk:StreamDescription" />
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>

          <xsd:element name="StopTransformResponse">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element minOccurs="0" maxOccurs="unbounded" name="streamDescription" type="agtk:StreamDescription" />
               </xsd:sequence>
            </xsd:complexType>
	  </xsd:element>

          <xsd:element name="StopTransform3Request">
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element minOccurs="0" maxOccurs="unbounded" name="streamDescription" type="agtk:StreamDescription3" />
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>

          <xsd:element name="StopTransform3Response">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element minOccurs="0" maxOccurs="unbounded" name="streamDescription" type="agtk:StreamDescription3" />
               </xsd:sequence>
            </xsd:complexType>
          </xsd:element>


      </schema>
      </types>


    <message name="TransformRequest">
	<part name="parameters" element="agnets:TransformRequest"/>
    </message>
    <message name="TransformResponse">
	<part name="parameters" element="agnets:TransformResponse"/>
    </message>
    <message name="StopTransformRequest">
	<part name="parameters" element="agnets:StopTransformRequest"/>
    </message>
    <message name="StopTransformResponse">
	<part name="parameters" element="agnets:StopTransformResponse"/>
    </message>

    <message name="Transform3Request">
	<part name="parameters" element="agnets:Transform3Request"/>
    </message>
    <message name="Transform3Response">
	<part name="parameters" element="agnets:Transform3Response"/>
    </message>
    <message name="StopTransform3Request">
	<part name="parameters" element="agnets:StopTransform3Request"/>
    </message>
    <message name="StopTransform3Response">
	<part name="parameters" element="agnets:StopTransform3Response"/>
    </message>

    <portType name="AGNetworkService">
    <operation name="Transform">
        <input message="agnets:TransformRequest"/>
        <output message="agnets:TransformResponse"/>
    </operation>
    <operation name="Transform3">
        <input message="agnets:Transform3Request"/>
        <output message="agnets:Transform3Response"/>
    </operation>
    <operation name="StopTransform">
        <input message="agnets:StopTransformRequest"/>
        <output message="agnets:StopTransformResponse"/>
    </operation>
    <operation name="StopTransform3">
        <input message="agnets:StopTransform3Request"/>
        <output message="agnets:StopTransform3Response"/>
    </operation>

    </portType>

</definitions>

